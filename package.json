{
  "name": "react-ollama",
  "version": "0.1.1",
  "description": "Integrate Ollama into your React application",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "exports": {
    "types": "./dist/index.d.mts",
    "import": "./dist/index.mjs"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint --report-unused-disable-directives --max-warnings 0 .",
    "lint-fix": "eslint --fix .",
    "format": "prettier --config .prettierrc --check --ignore-unknown .",
    "format-fix": "prettier --config .prettierrc --write --ignore-unknown ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/incandesc3nce/react-ollama.git"
  },
  "keywords": [
    "ollama",
    "react",
    "react hooks",
    "typescript"
  ],
  "author": "incandesc3nce <incandesc3ncee@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/incandesc3nce/react-ollama/issues"
  },
  "homepage": "https://github.com/incandesc3nce/react-ollama#readme",
  "dependencies": {
    "ollama": "^0.5.14"
  },
  "peerDependencies": {
    "react": ">= 17",
    "react-dom": ">= 17"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@vitest/coverage-v8": "^3.0.9",
    "eslint": "^9.23.0",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0",
    "vitest": "^3.0.9"
  }
}
